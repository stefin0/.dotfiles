{
    "modules-left": [
        "hyprland/workspaces",
    ],
    "modules-right": [
        "battery",
        "pulseaudio",
        "tray",
        "clock",
    ],
    "battery": {
        "bat": "BAT1",
        "format": "{capacity}% {icon} ",
        "format-charging": "{capacity}% {icon} ",
        "format-icons": {
            "default": ["󰁺", "󰁻", "󰁼", "󰁽", "󰁾", "󰁿", "󰂀", "󰂁", "󰂂", "󰁹"],
            "charging": ["󰢜", "󰂆", "󰂇", "󰂈", "󰢝", "󰂉", "󰢞", "󰂊", "󰂋", "󰂅"],
        },
    },
    "pulseaudio": {
        "format": "{volume}% {icon} ",
        "format-muted": "{volume}% 󰸈 ",
        "format-icons": {
          "default": ["󰕿", "󰖀", "󰕾"],
        },
        "on-click": "pavucontrol",
    },
    "tray": {
        "spacing": 10,
    },
    "clock": {
        "format": "{:%D %I:%M %p}",
        "tooltip-format": "<tt>{calendar}</tt>",
        "calendar": {
            "format": {
                "today": "<b><u>{}</u></b>",
            },
        },
    },
}
